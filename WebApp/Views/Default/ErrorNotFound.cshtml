@{
    ViewData["Title"] = "ErrorNotFound";
}

<section class="notfound-section">
    <div class="container">
        <div class="notfound-content">
            <img id="error-image" src="~/images/logos/404.svg" alt="404" />
            <div class="notfound-text">
            <h1>Oops!</h1>
            <p>The page you are looking for is not available.</p>
            </div>
            <a class="btn-theme" asp-action="Home" asp-controller="Default"><i class="fa-sharp fa-regular fa-house"></i>Go to homepage</a>
        </div>
    </div>
</section>
